#include <iostream>
#include<string.h>
#include<stdio.h>

using namespace std;

/*============Vari√°veis globais==============*/

int main()
{
	char _temp0[2];
	char _temp1[2];
	char _temp2[2];
	char _temp3[4];
	char _temp4[4];
	int _temp5;


	strcpy(_temp0, "ab");
	strcpy(_temp1, _temp0);

	strcpy(_temp2, "cd");
	strcpy(_temp3, _temp1);
	strcat(_temp3, _temp2);
	strcpy(_temp4, _temp3);


	cout << _temp4 << endl;

	_temp5 = 0;

	return _temp5;
}

