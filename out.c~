/*Compiler prescot-liller*/

#include <stdio.h>
#include <iostream>
#include<string.h>
#include<stdio.h>

using namespace std;

/*============Vari√°veis globais==============*/

int main()
{
	char _temp0[2];
	char _temp1[2];
	char _temp2[1];
	char _temp3[1];
	char _temp4[7];
	char _temp5[9];
	char _temp6[10];
	int _temp7;


	strcpy(_temp0, "ab");
	strcpy(_temp1, _temp0);

	strcpy(_temp2, "0");
	strcpy(_temp3, _temp2);

	strcpy(_temp4, "b+c+d: ");
	strcpy(_temp5, _temp4);

	cout << _temp5 << endl;
	
	strcat(_temp5, _temp1);

	cout << _temp5 << endl;

	strcpy(_temp6, _temp5);

	cout << _temp6 << endl;

	strcat(_temp6, _temp3);

	cout << _temp6 << endl;

	_temp7 = 0;

	return _temp7;
}

